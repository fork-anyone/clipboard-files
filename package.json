{
	"name": "@fork-anyone/clipboard-files",
	"version": "1.0.10",
	"description": "fork clipboard-files, A nodejs addon, read or write file path for clipboard, support win32 and mac osx",
	"keywords": [
		"clipboard",
		"files",
		"nwjs",
		"electron"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/alex8088/clipboard-files.git"
	},
	"license": "MIT",
	"author": "alex.wei",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"directories": {
		"lib": "lib",
		"test": "test"
	},
	"files": [
		"lib/",
		"src/",
		"binding.gyp",
		"README.md",
		"LICENSE"
	],
	"binary": {
		"module_name": "binding",
		"module_path": "./lib/binding/{node_abi}-{platform}-{arch}/",
		"remote_path": "v{version}",
		"package_name": "{node_abi}-{platform}-{arch}.tar.gz",
		"host": "https://localhost"
	},
	"scripts": {
		"build": "node-pre-gyp rebuild",
		"build-electron": "electron-rebuild .",
		"publishNpm": "npm publish --registry https://registry.npmjs.org/ --access public",
		"test": "node ./test"
	},
	"dependencies": {
		"nan": "^2.13.2"
	},
	"devDependencies": {
		"@mapbox/node-pre-gyp": "^1.0.11",
		"node-gyp": "^10.2.0"
	},
	"optionalDependencies": {
		"electron": "^22.3.13"
	},
	"volta": {
		"node": "16.17.1"
	},
	"gypfile": true
}
